<h2>CARGA DE ARCHIVO A DESCIFRAR</h2>
<div class="jumbotron">
    <h4 class="display-3"> Descifre su archivo mediante RSA</h4>
    <p class="lead">
        @using (Html.BeginForm("CargaParaDescifrar", "RSA", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
        <p>Sube el archivo a descifrar:</p>
        <input type="file" name="ADescifrar" />
        <hr />
        <p>Sube la clave para descifrar:</p>
        <input type="file" name="KeyDescifrar" />
        <hr />
        <input type="submit" value="Descifrar archivo" class="btn btn-outline-success" />
    }
        <h6>@ViewBag.Msg</h6>

    </div>